{
    "name": "dimsav/php-mysql-backup",
    "type": "library",
    "description": "",
    "keywords": ["backup","dropbox","php","mysql"],
    "homepage": "http://github.com/dimsav/php-mysql-backup",
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitrios Savvopoulos",
            "email": "ds@dimsav.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "": ["src/", "tests/"]
        }
    },
    "require": {
        "php": ">=5.3.0",
        "illuminate/config": "4.1.*",
        "illuminate/container": "4.1.*",
        "monolog/monolog": "1.*",
        "dimsav/unix-zipper": "1.*",
        "jakajancar/dropboxuploader": "1.1.12",
        "mccool/database-backup": "dev-master"
    },
    "require-dev": {
        "behat/behat": "2.5.*@stable",
        "phpunit/phpunit": "3.7.*",
        "phpspec/phpspec": "2.0.*@dev"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jakajancar/dropboxuploader",
                "version": "1.1.12",
                "source": {
                    "url": "https://github.com/jakajancar/DropboxUploader",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": ["DropboxUploader.php"]
                }
            }
        }
    ]
}